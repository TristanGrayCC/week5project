<h1>Mount a Steed!</h1>

<form action="/adoptions/new2" method="get">
  <p>
  Select an owner:
  </p>
  <p>
    <label>
      <select name="owner_id">
        <% for owner in @owners %>
        <option value="<%= owner.id %>"><%= owner.name %></option>
        <% end %>
      </select>
    </label>
  </p>

  <input type='submit' value="Next!">
</form>
